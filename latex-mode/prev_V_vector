# -*- mode: snippet -*-
# name: prev_V_vector
# key: V
# type: command
# --
(backward-sexp) (kill-sexp) (backward-word)
(if (= (preceding-char) ?\\ )
	(progn (message "yes") (kill-word 1)
		   (backward-delete-char 1) (insert "\\vec{\\")
		   (yank 1) (yank 2) (insert "}"))
(progn (message "no") (forward-word)
       (insert "\\vec{") (yank) (insert "}"))
)